<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.TilePane?>

<fx:root alignment="TOP_LEFT" prefHeight="150.0" prefWidth="200.0" type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
       <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0">
           <content>
               <TilePane fx:id="foldersTilePane" hgap="10.0" prefHeight="200.0" prefWidth="200.0" tileAlignment="TOP_LEFT" vgap="10.0">
                   <padding>
                       <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                   </padding></TilePane>
           </content>
       </ScrollPane>
   </children>
</fx:root>
